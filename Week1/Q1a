#include <iostream>
using namespace std;

int sumBetween(int first, int last) {
    int sum = 0;
    for (int i = first; i <= last; i++) {
        sum += i;
    }
    return sum;
}

int main() {
    int first, last;
    cout << "Enter first number: ";
    cin >> first;
    cout << "Enter last number: ";
    cin >> last;

    cout << "Sum = " << sumBetween(first, last) << endl;
    return 0;
}
